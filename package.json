{
	"name": "hidewallbot",
	"version": "1.0.0",
	"description": "Telegram bots that uses 12ft to generate paywall-free links",
	"main": "dist/bot.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TheEdoRan/hidewallbot.git"
	},
	"bugs": {
		"url": "https://github.com/theedoran/hidewallbot/issues"
	},
	"homepage": "https://github.com/theedoran/hidewallbot#readme",
	"keywords": [
		"telegram",
		"bot",
		"12ft",
		"paywall"
	],
	"scripts": {
		"prepare": "husky install",
		"dev": "ts-node-dev --rs ./src/bot.ts",
		"compile": "tsc",
		"commitlint": "commitlint",
		"lint-staged": "lint-staged",
		"typecheck": "tsc --noemit",
		"lint": "npm run typecheck && eslint . --ext .js,.ts --no-cache",
		"lint:fix": "npm run lint -- --fix"
	},
	"author": "TheEdoRan",
	"license": "MIT",
	"devDependencies": {
		"@commitlint/cli": "^17.4.4",
		"@commitlint/config-conventional": "^17.4.4",
		"@theedoran/eslint-config": "^1.0.8",
		"@types/node": "^18.14.2",
		"@types/valid-url": "^1.0.4",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"axios": "^1.3.4",
		"dotenv": "^16.0.3",
		"metascraper": "^5.33.8",
		"metascraper-image": "^5.33.7",
		"metascraper-title": "^5.33.7",
		"telegraf": "^4.11.2",
		"typegram": "^4.3.0",
		"valid-url": "^1.0.9"
	}
}
